# Source this file to setup env. variables for DBG proxy.

_proxy='http://webproxy.deutsche-boerse.de:8080/'

export http_proxy=$_proxy
export https_proxy=$_proxy
export ftp_proxy=$_proxy
export HTTP_PROXY=$_proxy
export HTTPS_PROXY=$_proxy
export FTP_PROXY=$_proxy

# ignore ssl cert verification for github
export GIT_SSL_NO_VERIFY=true

# vim:ft=sh
